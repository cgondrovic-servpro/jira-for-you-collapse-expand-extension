{
  "manifest_version": 2,
  "name": "Jira For You Swimlane Collapse / Expand",
  "version": "1.0.0",
  "description": "Collapse and expand sections on Jira for-you page based on swimlane statuses",
  "permissions": [
    "*://*.atlassian.net/*",
    "*://*.jira.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.atlassian.net/jira/for-you*", "*://*.jira.com/jira/for-you*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "32": "data:image/svg+xml;base64,",
    "64": "data:image/svg+xml;base64,"
  }
}